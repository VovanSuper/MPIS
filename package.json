  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app:client:dev": "cross-env ENV=development API_PORT=8080 concurrently \"npm start\" \"npm run json-server\"",
    "json-server": "json-server --port=8080 --w ./Server-mock/users.json -w ./Server-mock/events.json",
    "nest:build": "tsc -p Server/tsconfig.json",
    "nest:serve": "nodemon -e ts --watch Server/src --exec \"npm run nest:build && npm run nest:start\"",
    "nest:test": "mocha Server/src/**/*.spec.ts --require ts-node/register --reporter spec",
    "nest:start": "node Server/bin/server.js",
    "nest:dev:start": "node Server/index.dev.js",
    "nest:dev:start:watch": "nodemon",
    "app:dev": "cross-env ENV=development API_PORT=8080 concurrently \"npm start\" \"npm run nest:dev:start:watch\""
  },
